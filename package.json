{
  "name": "@4s1/eslint-config",
  "version": "3.2.0",
  "description": "ESLint config",
  "keywords": [
    "eslint",
    "prettier"
  ],
  "bugs": {
    "url": "https://gitlab.com/4s1/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/4s1/eslint-config.git"
  },
  "license": "MIT",
  "author": "Steffen <steffen@4s1.de>",
  "main": "./prettier.config.js",
  "files": [
    "prettier.config.js",
    ".eslintrc.yaml"
  ],
  "scripts": {
    "build": "echo no build script",
    "format": "prettier --write \"example/**/*.ts\"",
    "lbt": "pnpm run lint && pnpm run build && pnpm run test",
    "lint": "eslint --ext ts example/",
    "lint:fix": "npm run lint -- --fix",
    "test": "echo no tests",
    "release": "git diff --exit-code --quiet && pnpm run lbt && standard-version",
    "release:major": "pnpm run release -- --release-as major",
    "release:minor": "pnpm run release -- --release-as minor",
    "release:patch": "pnpm run release -- --release-as patch",
    "prepare": "husky install"
  },
  "prettier": "./prettier.config.js",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.2"
  },
  "devDependencies": {
    "@4s1/changelog-config": "3.0.0",
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@types/node": "14.17.26",
    "eslint": "8.0.0",
    "husky": "7.0.2",
    "prettier": "2.4.1",
    "standard-version": "9.3.1",
    "typescript": "4.4.4"
  },
  "peerDependencies": {
    "eslint": ">=7.32.0",
    "prettier": ">= 2.4.0"
  },
  "publishConfig": {
    "@4s1:registry": "https://gitlab.com/api/v4/projects/28730798/packages/npm/",
    "access": "public"
  }
}
