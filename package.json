{
  "name": "@4s1/eslint-config",
  "version": "3.1.2",
  "description": "ESLint config",
  "keywords": [
    "eslint",
    "prettier"
  ],
  "bugs": {
    "url": "https://gitlab.com/4s1/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/4s1/eslint-config.git"
  },
  "license": "MIT",
  "author": "Steffen <steffen@4s1.de>",
  "main": "./prettier.config.js",
  "files": [
    "prettier.config.js",
    ".eslintrc.yaml"
  ],
  "scripts": {
    "build": "echo no build script",
    "format": "prettier --write \"example/**/*.ts\"",
    "lbt": "pnpm run lint && pnpm run build && pnpm run test",
    "lint": "eslint --ext ts example/",
    "lint:fix": "npm run lint -- --fix",
    "semantic-release": "semantic-release",
    "test": "echo no tests"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "4.31.0",
    "@typescript-eslint/parser": "4.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2"
  },
  "devDependencies": {
    "@4s1/semantic-release-config": "2.1.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.1",
    "@semantic-release/gitlab": "6.2.2",
    "conventional-changelog": "3.1.24",
    "conventional-changelog-conventionalcommits": "4.6.1",
    "eslint": "7.32.0",
    "prettier": "2.4.0",
    "semantic-release": "17.4.7",
    "typescript": "4.4.3"
  },
  "peerDependencies": {
    "eslint": ">=7.32.0",
    "prettier": ">= 2.4.0"
  },
  "publishConfig": {
    "@4s1:registry": "https://gitlab.com/api/v4/projects/28730798/packages/npm/",
    "access": "public"
  }
}
